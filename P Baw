local features = window:AddTab("‡∏Ñ‡πà‡∏≤‡∏û‡∏•‡∏±‡∏á")
features:Show()

local player = game.Players.LocalPlayer
local leaderstats = player:WaitForChild("leaderstats")
local strengthStat = leaderstats:WaitForChild("Strength")
local rebirthsStat = leaderstats:WaitForChild("Rebirths")
local durabilityStat = player:WaitForChild("Durability")

local timeLabel = features:AddLabel("‚è±Ô∏èTime:")
timeLabel.TextSize = 24
timeLabel.Font = Enum.Font.PatrickHand

local stopwatchLabel = features:AddLabel("0d 0h 0m 0s")
stopwatchLabel.TextSize = 20
stopwatchLabel.Font = Enum.Font.PatrickHand

local function formatNumber(number)
local isNegative = number < 0
number = math.abs(number)
if number >= 1e15 then
    return (isNegative and "-" or "") .. string.format("%.2fQa", number / 1e15)
elseif number >= 1e12 then
    return (isNegative and "-" or "") .. string.format("%.2fT", number / 1e12)
elseif number >= 1e9 then
    return (isNegative and "-" or "") .. string.format("%.2fB", number / 1e9)
elseif number >= 1e6 then
    return (isNegative and "-" or "") .. string.format("%.2fM", number / 1e6)
elseif number >= 1e3 then
    return (isNegative and "-" or "") .. string.format("%.2fK", number / 1e3)
else
    return (isNegative and "-" or "") .. string.format("%.2f", number)
end
end

local projectedStrengthLabel = features:AddLabel("Strength Rate: 0 /Hour | 0 /Day | 0 /Week | 0 /Month")
projectedStrengthLabel.TextSize = 20

local projectedDurabilityLabel = features:AddLabel("Durability Rate: 0 /Hour | 0 /Day | 0 /Week | 0 /Month")
projectedDurabilityLabel.TextSize = 20

local projectedRebirthsLabel = features:AddLabel("Rebirths Rate: 0 /Hour | 0 /Day | 0 /Week | 0 /Month")
projectedRebirthsLabel.TextSize = 20

local lastStrengthUpdateTime = tick()
local lastDurabilityUpdateTime = tick()
local lastRebirthsUpdateTime = tick()
local lastStrengthValue = strengthStat.Value
local lastDurabilityValue = durabilityStat.Value
local lastRebirthsValue = rebirthsStat.Value

task.spawn(function()
    while true do
        local currentTime = tick()

        local strengthDelta = strengthStat.Value - lastStrengthValue
        local durabilityDelta = durabilityStat.Value - lastDurabilityValue
        local rebirthsDelta = rebirthsStat.Value - lastRebirthsValue

        local strengthTimeDelta = currentTime - lastStrengthUpdateTime
        local durabilityTimeDelta = currentTime - lastDurabilityUpdateTime
        local rebirthsTimeDelta = currentTime - lastRebirthsUpdateTime

        local strengthRatePerHour = (strengthDelta / strengthTimeDelta) * 3600
        local durabilityRatePerHour = (durabilityDelta / durabilityTimeDelta) * 3600
        local rebirthsRatePerHour = (rebirthsDelta / rebirthsTimeDelta) * 3600

        local formattedStrengthRate = formatNumber(strengthRatePerHour)
        local formattedDurabilityRate = formatNumber(durabilityRatePerHour)
        local formattedRebirthsRate = formatNumber(rebirthsRatePerHour)

        projectedStrengthLabel.Text = string.format("Strength Rate: %s /Hour | %s /Day | %s /Week | %s /Month",
                                                     formattedStrengthRate, formatNumber(strengthRatePerHour * 24),
                                                     formatNumber(strengthRatePerHour * 24 * 7), formatNumber(strengthRatePerHour * 24 * 30))

        projectedDurabilityLabel.Text = string.format("Durability Rate: %s /Hour | %s /Day | %s /Week | %s /Month",
                                                      formattedDurabilityRate, formatNumber(durabilityRatePerHour * 24),
                                                      formatNumber(durabilityRatePerHour * 24 * 7), formatNumber(durabilityRatePerHour * 24 * 30))

        projectedRebirthsLabel.Text = string.format("Rebirths Rate: %s /Hour | %s /Day | %s /Week | %s /Month",
                                                    formattedRebirthsRate, formatNumber(rebirthsRatePerHour * 24),
                                                    formatNumber(rebirthsRatePerHour * 24 * 7), formatNumber(rebirthsRatePerHour * 24 * 30))

        lastStrengthUpdateTime = currentTime
        lastDurabilityUpdateTime = currentTime
        lastRebirthsUpdateTime = currentTime
        lastStrengthValue = strengthStat.Value
        lastDurabilityValue = durabilityStat.Value 
        lastRebirthsValue = rebirthsStat.Value

        task.wait(6.5)
    end
end)
features:AddLabel("").TextSize = 10

local statsLabel = features:AddLabel("üìäStats:")
